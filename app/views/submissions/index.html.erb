<table>
  <tbody>
    <% @submissions.each do |submission| %>
      <tr class="post">
        <td>
          <%= link_to submission.title, submission, class: "title" %>
          <%= link_to submission.url, "(#{submission.url})", target: "_blank" %>
        </td>
        <tr class="post">
          <td> <p>325 points by <%= submission.user.email %> <%= time_ago_in_words(submission.created_at) %> | 96 comments </p></td>
        </tr>
      </tr>
    <% end %>
  </tbody>
</table>

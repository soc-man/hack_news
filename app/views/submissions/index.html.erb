<p style="color: green"><%= notice %></p>

<h1>Submissions</h1>

<div id="submissions">
  <% @submissions.each do |submission| %>
    <%= render submission %>
    <p>
      <%= link_to "Show this submission", submission %>
    </p>
  <% end %>
</div>
<% if user_signed_in? %>
  <%= link_to "New submission", new_submission_path %>
<% end %>
